{
    "name": "freedomsex/server-bundle",
    "type": "symfony-bundle",
    "description": "Meta `server` bundle for FreedomSex project api-servers",
    "license": "MIT",
    "repositories": [
        { "type": "git", "url": "git@github.com:freedomsex/auth-bundle" },
        { "type": "git", "url": "git@github.com:freedomsex/contracts" }
    ],
    "require": {
        "php": "^7.4.0",
        "freedomsex/client-identifier": "*",
        "symfony/orm-pack": "^2.0",
        "freedomsex/contracts": "*",
        "freedomsex/base-utilites": "*",
        "freedomsex/auth-bundle": "^0.4.8",
        "symfony/serializer": "^5.1",
        "symfony/twig-bundle": "^5.1",
        "symfony/security-bundle": "^5.1",
        "symfony/monolog-bundle": "^3.5",
        "sensio/framework-extra-bundle": "^5.6",
        "nelmio/cors-bundle": "^2.1",
        "symfony/validator": "^5.1",
        "freedomsex/health-bundle": "^1.0",
        "freedomsex/key-value-storage": "^2.0",
        "symfony/cache": "^5.1"
    },
    "require-dev": {
        "freedomsex/dev-pack": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "FreedomSex\\ServerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FreedomSex\\ServerBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "symfony": {
            "require": "5.1.*"
        }
    }
}
