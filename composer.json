{
    "name": "a4sex/server-bundle",
    "type": "symfony-bundle",
    "description": "Meta `server` bundle for FreedomSex project api-servers",
    "license": "MIT",
    "repositories": [
        { "type": "git", "url": "git@github.com:freedomsex/contracts" },
        { "type": "git", "url": "git@gitlab.com:a4sex/php/bundles/maintenance-bundle" }
    ],
    "require": {
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "composer/package-versions-deprecated": "*",
        "doctrine/annotations": "*",
        "doctrine/dbal": "*",
        "doctrine/doctrine-bundle": "*",
        "doctrine/doctrine-migrations-bundle": "*",
        "doctrine/orm": "*",
        "freedomsex/base-utilites": "*",
        "a4sex/client-identifier": "*",
        "freedomsex/contracts": "*",
        "a4sex/entity-cleaner-bundle": "*",
        "a4sex/entity-date-update": "*",
        "a4sex/health-bundle": "*",
        "a4sex/key-value-storage": "^2.0",
        "a4sex/maintenance-bundle": "*",
        "nelmio/cors-bundle": "^2.0",
        "phpdocumentor/reflection-docblock": "^5.0",
        "sensio/framework-extra-bundle": "*",
        "symfony/amqp-messenger": "^6.0",
        "symfony/cache": "^6.0",
        "symfony/console": "^6.0",
        "symfony/doctrine-messenger": "^6.2",
        "symfony/dotenv": "^6.0",
        "symfony/expression-language": "^6.0",
        "symfony/flex": "^1.9",
        "symfony/framework-bundle": "^6.0",
        "symfony/http-client": "^6.0",
        "symfony/messenger": "^6.0",
        "symfony/monolog-bundle": "^3.5",
        "symfony/property-access": "^6.0",
        "symfony/property-info": "^6.0",
        "symfony/proxy-manager-bridge": "^6.0",
        "symfony/runtime": "^6.0",
        "symfony/security-bundle": "^6.0",
        "symfony/serializer": "^6.0",
        "symfony/translation": "^6.0",
        "symfony/twig-bundle": "^6.0",
        "symfony/uid": "^6.0",
        "symfony/validator": "^6.0",
        "symfony/yaml": "^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "A4Sex\\ServerBundle\\": "src/"
        }
    }
}
