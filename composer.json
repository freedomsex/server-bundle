{
    "name": "freedomsex/server-bundle",
    "type": "symfony-bundle",
    "description": "Meta `server` bundle for FreedomSex project api-servers",
    "license": "MIT",
    "repositories": [
        { "type": "git", "url": "git@github.com:freedomsex/contracts" }
    ],
    "require": {
        "php": "^7.4.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/dbal": "^2.6",
        "freedomsex/base-utilites": "*",
        "freedomsex/client-identifier": "*",
        "freedomsex/contracts": "*",
        "freedomsex/health-bundle": "*",
        "freedomsex/key-value-storage": "^2.0",
        "nelmio/cors-bundle": "^2.0",
        "sensio/framework-extra-bundle": "^5.0",
        "symfony/amqp-messenger": "^5.0",
        "symfony/cache": "^5.0",
        "symfony/console": "^5.0",
        "symfony/doctrine-messenger": "^5.2",
        "symfony/dotenv": "^5.0",
        "symfony/expression-language": "^5.0",
        "symfony/flex": "^1.9",
        "symfony/framework-bundle": "^5.0",
        "symfony/http-client": "^5.0",
        "symfony/messenger": "^5.2",
        "symfony/monolog-bundle": "^3.5",
        "symfony/orm-pack": "^2.0",
        "symfony/security-bundle": "^5.0",
        "symfony/serializer": "^5.0",
        "symfony/runtime": "^5.0",
        "symfony/twig-bundle": "^5.0",
        "symfony/uid": "^5.0",
        "symfony/validator": "^5.0",
        "symfony/yaml": "^5.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require-dev": {
        "freedomsex/dev-pack": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "FreedomSex\\ServerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FreedomSex\\ServerBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "symfony": {
            "require": "5.3.*"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
