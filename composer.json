{
    "name": "freedomsex/server-bundle",
    "type": "symfony-bundle",
    "description": "Meta `server` bundle for FreedomSex project api-servers",
    "license": "MIT",
    "repositories": [
        { "type": "git", "url": "git@github.com:freedomsex/contracts" }
    ],
    "require": {
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "composer/package-versions-deprecated": "*",
        "doctrine/annotations": "*",
        "doctrine/dbal": "*",
        "doctrine/doctrine-bundle": "*",
        "doctrine/doctrine-migrations-bundle": "*",
        "doctrine/orm": "*",
        "freedomsex/base-utilites": "*",
        "freedomsex/client-identifier": "*",
        "freedomsex/contracts": "*",
        "freedomsex/entity-date-update": "*",
        "freedomsex/health-bundle": "*",
        "freedomsex/key-value-storage": "^2.0",
        "nelmio/cors-bundle": "^2.0",
        "phpdocumentor/reflection-docblock": "^5.0",
        "sensio/framework-extra-bundle": "*",
        "symfony/amqp-messenger": "^5.0",
        "symfony/cache": "^5.0",
        "symfony/console": "^5.0",
        "symfony/doctrine-messenger": "^5.2",
        "symfony/dotenv": "^5.0",
        "symfony/expression-language": "^5.0",
        "symfony/flex": "^1.9",
        "symfony/framework-bundle": "^5.0",
        "symfony/http-client": "^5.0",
        "symfony/messenger": "^5.2",
        "symfony/monolog-bundle": "^3.5",
        "symfony/property-access": "^5.0",
        "symfony/property-info": "^5.0",
        "symfony/proxy-manager-bridge": "^5.0",
        "symfony/runtime": "^5.0",
        "symfony/security-bundle": "^5.0",
        "symfony/serializer": "^5.0",
        "symfony/translation": "^5.0",
        "symfony/twig-bundle": "^5.0",
        "symfony/uid": "^5.0",
        "symfony/validator": "^5.0",
        "symfony/yaml": "^5.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "FreedomSex\\ServerBundle\\": "src/"
        }
    }
}
